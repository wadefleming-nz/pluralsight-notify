{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/svg/**"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "emergency-calls",
      "urls": ["https://us-central1-ps-notify-api.cloudfunctions.net/api/*"],
      "cacheConfig": {
        "maxSize": 250,
        "maxAge": "30d",
        "strategy": "performance"
      }
    },
    {
      "name": "latest",
      "urls": [
        "https://us-central1-ps-notify-api.cloudfunctions.net/api/latest"
      ],
      "cacheConfig": {
        "maxSize": 1,
        "maxAge": "1d",
        "timeout": "2s",
        "strategy": "freshness"
      }
    },
    {
      "name": "acknowledgements",
      "urls": [
        "https://us-central1-ps-notify-api.cloudfunctions.net/api/acknowledgements/*"
      ],
      "cacheConfig": {
        "maxSize": 500,
        "maxAge": "1d",
        "timeout": "2s",
        "strategy": "freshness"
      }
    }
  ]
}
